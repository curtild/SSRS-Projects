  <DataSets>
    <DataSet Name="rsd_Countries">
      <Query>
        <DataSourceName>srds_AdventureWorks2017</DataSourceName>
        <CommandText>SELECT DISTINCT CountryRegionCode as Country_ID,
CountryName AS Country
FROM v_StoresByCity
GROUP BY CountryRegionCode, CountryName, StateName, City
HAVING Count(*)&gt;=10;</CommandText>
      </Query>
      <Fields>
        <Field Name="Country_ID">
          <DataField>Country_ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Country">
          <DataField>Country</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="rsd_States">
      <Query>
        <DataSourceName>srds_AdventureWorks2017</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CountryCode">
            <Value>=Parameters!CountryCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT StateProvinceCode AS State_Code,
StateName
FROM v_StoresByCity
WHERE CountryRegionCode=@CountryCode
GROUP BY CountryName, CountryRegionCode, StateProvinceCode,
StateName, City
HAVING Count(*)&gt;=10;</CommandText>
      </Query>
      <Fields>
        <Field Name="State_Code">
          <DataField>State_Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StateName">
          <DataField>StateName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="rsd_Cities">
      <Query>
        <DataSourceName>srds_AdventureWorks2017</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StateProvinceCode">
            <Value>=Parameters!StateProvinceCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT City, StateProvinceCode
FROM v_StoresByCity
WHERE StateProvinceCode=@StateProvinceCode
GROUP BY StateProvinceCode, City
HAVING Count(*)&gt;=10;</CommandText>
      </Query>
      <Fields>
        <Field Name="City">
          <DataField>City</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StateProvinceCode">
          <DataField>StateProvinceCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
