        <DataSourceName>srds_AdventureWorks2017</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DEPARTMENT">
            <Value>=Parameters!DEPARTMENT.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
RECORD_NO,
FIRSTNAME,
MIDDLENAME,
LASTNAME,
EMPLOYEE,
JOBTITLE,
DEPARTMENT,
BARCODE_TEXT,
BARCODE_IMAGE
FROM v_EMPLOYEE_BARCODES
WHERE DEPARTMENT = @DEPARTMENT;</CommandText>
